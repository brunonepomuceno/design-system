        "$border-radius-circular": "50%"
      },
      "style": {
        "$border-style-solid": "solid",
        "$border-style-dashed": "dashed",
        "$border-style-dotted": "dotted"
      },
      "color": {
        "$border-color-neutral-default": "#DDDFE4",
        "$border-color-neutral-subtle": "#BABEC9",
        "$border-color-neutral-strong": "#636B7E",
        "$border-color-brand-default": "#E04747",
        "$border-color-brand-strong": "#B21919",
        "$border-color-error-default": "#E04747",
        "$border-color-warning-default": "#F5963D",
        "$border-color-success-default": "#0AB15F",
        "$border-color-info-default": "#3C82FF",
        "$border-color-focus": "#3C82FF"
      },
      "preset": {
        "$border-default": "1px solid #DDDFE4",
        "$border-subtle": "1px solid #BABEC9",
        "$border-strong": "1px solid #636B7E",
        "$border-brand": "1px solid #E04747",
        "$border-error": "1px solid #E04747",
        "$border-warning": "1px solid #F5963D",
