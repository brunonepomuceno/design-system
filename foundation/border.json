          "$border-alert-warning": "1px solid #FFF8D6",
          "$border-alert-error": "1px solid #FFF4F4"
        }
      }
    },
    "mapping": {
      "stroke-hjh57h5": "$border-strong",
      "stroke-hjglevs": "$border-subtle",
      "stroke-hb4fe40": "$border-default",
      "stroke-hji8ecv": "$border-strong",
