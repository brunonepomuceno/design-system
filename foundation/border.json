{
  "border": {
    "semantic": {
      "width": {
        "$border-width-none": "0",
        "$border-width-hairline": "0.5px",
        "$border-width-thin": "1px",
        "$border-width-thick": "2px",
        "$border-width-heavy": "4px"
      },
      "radius": {
        "$border-radius-none": "0",
